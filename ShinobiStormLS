local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/NICKISBAD/Nick-s-Modded-KAVO-Lib/main/Nick'sModdedKavoLib.lua"))()
local Window = Library.CreateLib("Shinobi Storm", "Ocean")
local Tab = Window:NewTab("Character Unlock")
local Section = Tab:NewSection("Main")
Section:NewLabel("do NOT spam or you will get kicked")
Section:NewLabel("wait after resetting to unlock a new character")
Section:NewLabel("it saves btw")
Section:NewButton("Unlock Shisui", "Unlock", function()
  local player = game.Players.LocalPlayer
local shopValuesFolder = player:WaitForChild("ShopValues")
local shisuiValue = shopValuesFolder:WaitForChild("Shisui")

shisuiValue.Value = true

local args = {
    [1] = "Shisui"
}

game:GetService("ReplicatedStorage").PickedShop:FireServer(unpack(args))

wait(2)
player.Character.Humanoid.Health = 0
    print("Clicked")
end)

